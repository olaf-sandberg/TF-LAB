{
  "version": "2021.12",
  "core-network-configuration": {
    "asn-ranges": ["64512-65534"],
    "edge-locations": [
      { "location": "eu-central-1" },
      { "location": "eu-west-1" },
      { "location": "eu-west-2" }
    ],
    "segments": [
      {
        "name": "DEV",
        "description": "Development segment"
      },
      {
        "name": "PROD",
        "description": "Production segment"
      },
      {
        "name": "SHARED",
        "description": "Shared services segment"
      }
    ],
    "segment-actions": [
      {
        "action": "share",
        "mode": "attachment-route",
        "segment": "DEV",
        "share-with": ["SHARED"]
      },
      {
        "action": "share",
        "mode": "attachment-route",
        "segment": "PROD",
        "share-with": ["SHARED"]
      }
    ]
  },
  "segments": [
    {
      "name": "DEV"
    },
    {
      "name": "PROD"
    },
    {
      "name": "SHARED"
    }
  ]
}
