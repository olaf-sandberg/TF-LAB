{
  "version": "2024.11",
  "core-network-configuration": {
    "asn-ranges": ["64512-64520"],
    "edge-locations": [
      { "location": "eu-central-1" },
      { "location": "eu-west-1" },
      { "location": "eu-west-2" }
    ],
    "segments": [
      {
        "name": "shared",
        "require-attachment-acceptance": false,
        "isolate-attachments": false
      },
      {
        "name": "dev",
        "require-attachment-acceptance": false,
        "isolate-attachments": true
      },
      {
        "name": "prod",
        "require-attachment-acceptance": false,
        "isolate-attachments": true
      }
    ]
  },
  "network-segments": [
    {
      "name": "shared",
      "allowed-customer-segments": ["dev", "prod"]
    },
    {
      "name": "dev",
      "allowed-customer-segments": ["shared"]
    },
    {
      "name": "prod",
      "allowed-customer-segments": ["shared"]
    }
  ]
}
